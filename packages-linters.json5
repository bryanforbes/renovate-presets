{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "description": "Linters and formatters",
  "extends": [
    "packages:linters"
  ],
  "matchPackageNames": [
    "@types/eslint-**",
    "@vitest/eslint-plugin",
    "globals",
    "prettier-plugin-**",
    "@prettier/plugin-**",
    "husky",
    "@commitlint/**",
    "lint-staged",
    "@types/lint-staged"
  ]
}
