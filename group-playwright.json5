{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "description": "Group Playwright packages",
  "packageRules": [
    {
      "extends": [
        "github>bryanforbes/renovate-presets:packages-playwright.json5"
      ],
      "groupName": "Playwright"
    },
    {
      "description": "Disable minimum release age checks for playwright docker image",
      "matchPackageNames": [
        "mcr.microsoft.com/playwright"
      ],
      "minimumReleaseAge": null
    }
  ]
}
